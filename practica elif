a = [3, (1,3), [4,5,6], 7, "hola", '0',0, 1]
if 'a' in a:
    print("a es dins la llista {}".format(a))
elif 'holas' in a:
    print("hola es dins la llista {}".format(a))
elif '0b' in a:
    print("Aixo s'executa si es certa la condicio")
elif 'b' in a:
    print("b es dins {}".format(a))
else:
    print("Dins a hi ha {}".format(a))



